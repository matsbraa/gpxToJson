{
  "name": "electronConvertGdxToJSON",
  "productName": "GPX to JSON converter",
  "version": "0.9.0",
  "description": "A Gpx to JSON converter",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=box.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=box.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"A Gpx to JSON converter\"",
    "package-linux" : "electron-packager . --overwrite --platform=linux --arch=x64 --icon=abox.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "Mats Braa",
  "license": "MIT",
  "devDependencies": {
    "electron-prebuilt": "^1.4.13",
    "electron-packager": "^8.5.1"
  }
}
